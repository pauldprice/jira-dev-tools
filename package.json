{
  "name": "@gather/toolbox",
  "version": "1.0.0",
  "description": "A collection of TypeScript tools for development workflow automation",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node",
    "build": "tsc",
    "clean": "rm -rf dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --selectProjects=node",
    "test:react": "jest --selectProjects=react",
    "test:integration": "jest --testMatch='**/*.integration.test.ts'",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "fetch-jira": "ts-node src/tools/fetch-jira-ticket.ts",
    "fetch-attachment": "ts-node src/tools/fetch-jira-attachment.ts",
    "release-notes": "ts-node src/tools/generate-release-notes.ts",
    "toolbox": "./toolbox"
  },
  "keywords": [
    "tools",
    "automation",
    "jira",
    "release-notes",
    "git"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.20.0",
    "@types/inquirer": "^8.2.11",
    "@types/pg": "^8.15.4",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "axios": "^1.6.0",
    "chalk": "^4.1.2",
    "commander": "^11.0.0",
    "csv-writer": "^1.6.0",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "dotenv-expand": "^10.0.0",
    "inquirer": "^8.2.6",
    "inquirer-autocomplete-prompt": "^2.0.1",
    "ora": "^5.4.1",
    "pg": "^8.16.2",
    "puppeteer": "^24.10.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "simple-git": "^3.19.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/inquirer-autocomplete-prompt": "^2.0.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.8.0",
    "jest": "^30.0.0",
    "jest-environment-jsdom": "^30.0.0",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
